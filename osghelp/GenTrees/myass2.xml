<GEN
  uselights="000000001"
  no_geo="true"
  >

  <TEXTURE_TREE 
    suppress="false"
    unit="0"
  />
  
  <TREE 
    sizemultiplier="4.0" 
    radius_between_trees="1.0"
    min="3.0"
    max="5.0"
  />
  
  <TERRAIN 
    debug="true"
    gridx="2"
    gridy="2"
    cellw="1000"
    cellh="1000"
    subx="5"
    suby="5"
  />

  <SHAPE_QUAD
    w="1000"
    h="1000"
    offsetz="5"
    renderme="false"
  />

  <!--
  <UNIFORM name="use_tree_shader" value="1"     type="INT"/>
  <UNIFORM name="tex"             value="0"     type="SAMPLER_2D"/>
  <UNIFORM name="texPositions"    value="1"     type="SAMPLER_2D"/>
  <UNIFORM name="texSizeSquared"  value="8"     type="INT"/>
  <UNIFORM name="numInstances"    value="20"    type="INT"/>
  <UNIFORM name="minSize"         value="3"     type="FLOAT"/>
  <UNIFORM name="maxSize"         value="5"     type="FLOAT"/>
  -->


  <SHADER type="vertex"   file="shaders/funcTree.vert"/>
  <SHADER type="vertex"   file="shaders/funcLightingOpenGL.vert"/>
  <SHADER type="vertex"   file="shaders/funcLighting.vert"/>
  <SHADER type="fragment" file="shaders/funcFogOpenGL.frag"/>
  <SHADER type="fragment" file="shaders/funcLighting.frag"/>
  <SHADER type="fragment" file="shaders/funcLightingOpenGL.frag"/>
  <SHADER type="vertex"   file="shaders/main_standard.vert"/>
  <SHADER type="fragment" file="shaders/main_standard.frag"/>
  
  <OUTPUT file="myass.ive" />
  
  <VIEWER suppress="false" />
  
  <DEBUGGING 
    showpivots="true"
  />
    
</GEN>


